{
    "name": "@nodescript/stdlib",
    "verison": "0.0.0",
    "type": "module",
    "description": "Standard Node Definitions",
    "exports": {
        "./nodes/Array.js": "./out/nodes/Array.js",
        "./nodes/Boolean.js": "./out/nodes/Boolean.js",
        "./nodes/KeyValue.js": "./out/nodes/KeyValue.js",
        "./nodes/Number.js": "./out/nodes/Number.js",
        "./nodes/Object.js": "./out/nodes/Object.js",
        "./nodes/String.js": "./out/nodes/String.js"
    },
    "files": [
        "out/lib/**/*",
        "out/nodes/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "run-s clean dev:ts",
        "dev:ts": "tsc -b -w",
        "compile": "run-s compile:*",
        "compile:ts": "tsc -b",
        "lint": "eslint --ext=.js,.ts --cache .",
        "test": "NODE_ENV=test mocha",
        "preversion": "npm run lint",
        "version": "run-s compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "engines": {
        "node": ">=18"
    },
    "pre-commit": [
        "lint"
    ],
    "author": "UBIO",
    "dependencies": {
        "@nodescript/core": "^0.2.1"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.35",
        "@ubio/eslint-config": "^1.3.5",
        "eslint": "^7.32.0",
        "mocha": "^10.0.0",
        "npm-run-all": "^4.1.5",
        "typescript": "^4.6.4"
    },
    "version": "0.0.2"
}
